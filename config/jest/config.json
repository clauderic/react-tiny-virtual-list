{
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "rootDir": "../..",
  "roots": [
    "<rootDir>/tests"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "transform": {
    "\\.tsx?$": "<rootDir>/config/jest/transformers/typescript.js"
  },
  "testMatch": [
    "**/?(*.)+(spec|test).+(ts|tsx|js)"
  ]
}
